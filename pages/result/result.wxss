/* pages/result/result.wxss */
page{
  background-color: #dfdfdf;
}
.h1{
  text-align: center;
  font-size: 50rpx;
  padding: 20px 0 0;
}
.h2{
  font-size: 40rpx;
  text-indent: 5px;
  padding: 10px 0;
}
.h3{
  font-size: 22rpx;
  text-align: center;
  padding-bottom: 20px;
  color: #999
}
.continer{
  padding-left: 10px;
  padding-right: 10px;
}
.quest{
  display: flex;
  flex-direction: column;
  margin-bottom: 10px;
}
.title{
  border-top-left-radius: 10px;
  border-top-right-radius: 10px;
  background-color: #fff;
  border-bottom: 1px dashed #c6c6c6;
  padding: 10px 20px;
  color: #a40000;
}
.cont{
  border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10px;
  background-color: #fff;
  padding: 10px 20px;
  color: rgb(91, 91, 91);
}